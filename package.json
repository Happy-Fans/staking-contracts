  "license": "MIT",
  "name": "Staking Contracts",
  "version": "2.0.0",
  "description": "Staking Smart Contracts that allow users to stake tokens in exchange for rewards",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.0.4",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/test-helpers": "^0.5.10",
    "dotenv": "^8.2.0",
    "ethers": "^5.1.2",
    "hardhat": "^2.6.6",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.7.5",
    "solhint": "^3.3.4",
    "solidity-coverage": "^0.7.16",
    "web3": "^1.3.5"
  },
  "scripts": {
    "build": "npx hardhat compile",
    "deploy_locked": "npx hardhat run --network {network_name} ../deploy-locked-staking.js",
    "deploy_lp": "npx hardhat run --network {network_name} ../deploy-lp-staking.js",
    "deploy_single": "npx hardhat run --network {network_name} ../deploy-single-staking.js",
    "verify": "npx hardhat verify --network {network_name} {contract_address} {args}",
    "test": "npx hardhat test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Happy-Fans/staking-contracts.git"
  }
}
